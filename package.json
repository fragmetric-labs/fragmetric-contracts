{
  "name": "@fragmetric-labs/contracts",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=20"
  },
  "packageManager": "pnpm@10.7.0",
  "scripts": {
    "lint": "prettier \"**/*.{ts,mjs,cjs}\" --write",
    "lint:check": "prettier \"**/*.{ts,mjs,cjs}\" --check",
    "test": "pnpm vitest --run",
    "test:watch": "pnpm vitest --ui",
    "test:coverage": "pnpm vitest --coverage --run",
    "build": "pnpm -r build",
    "codegen": "pnpm --filter ./clients/codegen build",
    "keypairs:download": "cd keypairs && (which jq 1>/dev/null || brew install jq) && aws secretsmanager get-secret-value --profile encrypt_dev --secret-id wallet --region ap-northeast-2 --output json | jq .SecretString | jq -c fromjson | jq -r 'to_entries[] | \"\\(.key) \\(.value)\"' | while read -r key value; do echo $value > $key; done",
    "dev": "pnpm dev:restaking",
    "dev:restaking": "./programs/restaking/tests/utils/cli.executable.ts connect --keypairs ./keypairs ./keypairs/restaking ledger",
    "connect": "./clients/js/fragmetric-sdk/src/cli/cli.executable.ts connect --keypairs ./keypairs/wallet.json ./keypairs/restaking ledger"
  },
  "devDependencies": {
    "@fragmetric-labs/sdk": "workspace:*",
    "@fragmetric-labs/testutil": "workspace:*",
    "@vitest/coverage-v8": "^3.1.1",
    "@vitest/ui": "^3.1.1",
    "prettier": "^3.5.3",
    "prettier-plugin-organize-imports": "^4.1.0",
    "tsx": "^4.19.3",
    "typescript": "^5.8.2",
    "vitest": "^3.1.1",
    "ws": "^8.18.1"
  }
}
