{
  "name": "@fragmetric-labs/sdk",
  "version": "0.0.1",
  "description": "The Client SDK for Fragmetric Solana programs serves both end users and operational administrators.\nIt is designed for compatibility with web browsers and Node.js environments, offering support for web browser bundles (in ESM, CJS, and UMD formats) as well as Node.js bundles (in ESM and CJS formats).",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fragmetric-labs/fragmetric-contracts.git"
  },
  "author": "Fragmetric Labs",
  "license": "MIT",
  "private": false,
  "types": "lib/index.d.ts",
  "main": "lib/index.cjs.js",
  "module": "lib/index.esm.js",
  "browser": {
    "lib/index.cjs.js": "lib/index.browser.cjs.js",
    "lib/index.esm.js": "lib/index.browser.esm.js"
  },
  "files": [
    "lib",
    "examples",
    "README.md"
  ],
  "scripts": {
    "build": "rollup --config",
    "build:node": "BUILD_NODE_ONLY=1 rollup --config",
    "test": "echo \"noop\"",
    "dev": "rollup --config --watch",
    "dev:node": "BUILD_NODE_ONLY=1 rollup --config --watch"
  },
  "dependencies": {
    "@coral-xyz/anchor": "^0.30.1",
    "bn.js": "^5.2.1"
  },
  "peerDependencies": {
    "@ledgerhq/hw-app-solana": "^7.2.4",
    "@ledgerhq/hw-transport": "^6.31.4",
    "@ledgerhq/hw-transport-node-hid-singleton": "^6.31.5",
    "@solana/web3.js": "^1.94.0"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^5.1.1",
    "@rollup/plugin-commonjs": "^28.0.2",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@rollup/plugin-replace": "^6.0.2",
    "rollup": "^4.29.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.36.0"
  }
}
